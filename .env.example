# Production environment variables for docker-compose.prod.yml

# Database
DB_USER=postgres
DB_PASSWORD=your-secure-password-here

# JWT
JWT_SECRET=your-very-secure-jwt-secret-key-here-minimum-32-characters

# CORS
ALLOWED_ORIGINS=https://your-domain.com

# Frontend API URL (for production builds)
FRONTEND_API_URL=https://your-domain.com/api